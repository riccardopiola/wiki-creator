{
  "name": "react-wikipage-creator",
  "version": "2.0.0",
  "description": "Simple MarkDown wiki files generator for React Components APIs",
  "main": "cli.js",
  "bin": "cli.js",
  "files": [
    "src",
    "github.js",
    "cli.js",
    "lib"
  ],
  "scripts": {
    "build": "flow-remove-types src/ --pretty -d lib/ && cp -R src/Templates/ lib/Templates/",
    "build-test": "flow-remove-types src/ --pretty -d testing/src/ && cp -R src/Templates/ testing/src/Templates/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/riccardopiola/react-wikipage-creator.git"
  },
  "keywords": [
    "react",
    "wiki",
    "github wiki",
    "documentation"
  ],
  "authors": [
    "Riccardo Piola <riccardopiola@live.it> (https://github.com/riccardopiola)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjs/riccardopiola/react-wikipage-creator/issues"
  },
  "dependencies": {
    "fs-extra": "^4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.56.0",
    "flow-remove-types": "^1.2.1",
    "jest": "^21.2.1"
  },
  "npmName": "react-wikipage-creator"
}
